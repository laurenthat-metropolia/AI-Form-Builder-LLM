  "scripts": {
    "build:api": "nx build api",
    "build:web": "nx build web",
    "build:llm": "nx build llm",
    "migraiton:deploy": "npx prisma migrate deploy --schema=./apps/api/prisma/schema.prisma",
    "migraiton:dev": "npx prisma migrate dev --schema=./apps/api/prisma/schema.prisma",
    "formward:db": "kubectl port-forward applications-postgresql-0 --namespace=databases 5432:5432",
    "start:api": "nx serve api --inspect false",
    "start:web": "nx serve web --port 8443",
    "start:llm": "nx serve llm",
    "op:start:api": "APP_ENV=local op run --no-masking --env-file=apps/api/.env.op npm run start:api",
    "op:start:web": "APP_ENV=local op run --no-masking --env-file=apps/web/.env.op npm run start:web",
    "op:start:llm": "APP_ENV=local op run --no-masking --env-file=apps/llm/.env.op npm run start:llm",
    "postinstall": "npx prisma generate --schema=./apps/api/prisma/schema.prisma"
  },
