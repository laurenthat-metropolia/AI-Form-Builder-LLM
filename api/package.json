{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=development npx ts-node --esm --project tsconfig.json app/main.ts",
    "watch": "nodemon --watch './**/*' -e '*' --exec 'npm run start'",
    "watch:op": "APP_ENV=local op run --no-masking --env-file=.env.op npm run watch",
    "op:db:migrate:dev": "APP_ENV=local op run --no-masking --env-file=.env.op npx prisma migrate dev",
    "op:db:migrate:deploy": "APP_ENV=local op run --no-masking --env-file=.env.op npx prisma migrate deploy",
    "watch:deploy": "cat deployment.yml  | APP_ENV=prod op inject > deployment.injected.yml && nodemon --watch './**/*' -e '*'  --delay 2.5 --exec 'npm run docker:build && npm run docker:push && npm run docker:deploy'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.449.0",
    "@prisma/client": "^5.5.2",
    "canvas": "^2.11.2",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-fetch": "^3.3.2",
    "openai": "^4.17.4",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/express-session": "^1.17.8",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/morgan": "^1.9.6",
    "@types/multer": "^1.4.10",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.8.0",
    "@types/passport": "^1.0.13",
    "@types/passport-google-oauth2": "^0.1.6",
    "@types/passport-jwt": "^3.0.10",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
